<%= simple_form_for(@progress_report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :workorder_id %>
    <%= f.input :quantity %>
    <%= f.input :user_id %>
    <%= f.input :notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

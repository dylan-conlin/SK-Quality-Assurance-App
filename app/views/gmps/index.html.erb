<h1>Listing GMPs</h1>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="prettytable">

<thead>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
</thead>
<tbody>
<% @gmps.each do |gmp| %>
  <tr>
    <td><%= gmp.name %></td>
    <td><%= link_to 'Show', gmp %></td>
    <td><%= link_to 'Edit', edit_gmp_path(gmp) %></td>
    <td><%= link_to 'Destroy', gmp, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</tbody>
</table>

<br />

<%= link_to 'New Gmp', new_gmp_path %>
